<?xml version="1.0" encoding="utf-8"?>
  <!--实心圆角矩形-->
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">
    <!--描边-->
    <stroke
        android:width="0.5dp"
        android:color="@color/homered"/>
    <!--圆角-->
    <corners android:radius="2dp"/>

    <!--填充颜色-透明-->
    <solid android:color="@color/homered"/>

</shape>